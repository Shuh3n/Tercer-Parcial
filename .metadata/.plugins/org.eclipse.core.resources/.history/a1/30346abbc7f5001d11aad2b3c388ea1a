package programacion2.parcial3.model;

public class Empresa {

	private String nombre;

	public Empresa() {
		super();
	}

	public Empresa(String nombre) {
		super();
		this.nombre = nombre;
	}

	public String getNombre() {
		return nombre;
	}

	public void setNombre(String nombre) {
		this.nombre = nombre;
	}
	
	

}
